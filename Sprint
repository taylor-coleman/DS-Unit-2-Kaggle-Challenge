        "from sklearn.ensemble import RandomForestClassifier\n",
        "import category_encoders as ce\n",
        "\n",
        "pipeline = make_pipeline(ce.OrdinalEncoder(), RandomForestClassifier(n_estimators = 10, min_samples_leaf = 9, max_depth = 33, n_jobs = -1))\n",
        "\n",
        "pipeline.fit(X_train, y_train)"
      ],
